<div id="<%= dom_id tenant %>">

  <p>
    <strong>First name:</strong>
    <%= tenant.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= tenant.last_name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= tenant.email %>
  </p>

  <p>
    <strong>Home district:</strong>
    <%= tenant.home_district %>
  </p>

  <p>
    <strong>Village:</strong>
    <%= tenant.village %>
  </p>

  <p>
    <strong>Next of kin:</strong>
    <%= tenant.next_of_kin %>
  </p>

  <p>
    <strong>Room:</strong>
    <%= tenant.room&.room_number %>
  </p>

  <p>
  <strong>Room:</strong>
  <%= tenant.room&.price %>
</p>

<p>
<strong>Room type:</strong>
<%= tenant.room.room_type.status_name %>
</p>

</div>
